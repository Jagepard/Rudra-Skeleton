{
    "name"             : "rudra/skeleton",
    "description"      : "Rudra Skeleton",
    "type"             : "library",
    "keywords"         : [
        "rudra",
        "framework",
        "mvc-framework",
        "mvc-pattern",
        "mvc-architecture",
        "mvc"
    ],
    "license"          : "MIT",
    "authors"          : [
        {
            "name"    : "Jagepard",
            "email"   : "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role"    : "Developer"
        }
    ],
    "support"          : {
        "email": "jagepard@yandex.ru",
        "skype": "jagepard.ru"
    },
    "autoload"         : {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "require"          : {
        "php"                    : ">=7.1",
        "twig/twig"              : "~1.0",
        "kint-php/kint"          : "^1.0",
        "maximebf/debugbar"      : "1.*",
        "symfony/yaml"           : "*",
        "robmorgan/phinx"        : "^0.9.1",
        "fzaninotto/faker"       : "^1.7",
        "swiftmailer/swiftmailer": "^6.0",
        "filp/whoops"            : "^2.2@dev"
    },
    "require-dev"      : {
        "rudra/connector"           : "dev-master",
        "rudra/exception"           : "dev-master",
        "rudra/controller"          : "dev-master",
        "rudra/auth"                : "dev-master",
        "rudra/pagination"          : "dev-master",
        "rudra/router"              : "dev-master",
        "rudra/container"           : "dev-master",
        "rudra/validation"          : "dev-master",
        "rudra/redirect"            : "dev-master",
        "rudra/model"               : "dev-master",
        "rudra/annotation"          : "dev-master",
        "rudra/event-dispatcher"    : "dev-master",
        "rudra-bundle-jagepard/auth": "dev-master",
        "codeception/codeception"   : "2.5.x-dev"
    },
    "repositories"     : [
        {
            "type": "vcs",
            "url" : "git@github.com:Jagepard/Rudra-AuthBundle.git"
        }
    ],
    "minimum-stability": "dev",
    "config"           : {
        "optimize-autoloader": true,
        "prefer-stable"      : true
    },
    "extra"            : {
        "installer-paths": {
            "app/{$name}": [
                "type:zend-module"
            ]
        }
    }
}
